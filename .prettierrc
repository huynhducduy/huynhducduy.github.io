{
  "trailingComma": "es5",
  "tabWidth": 2,
  "endOfLine": "lf",
  "printWidth": 80,
  "useTabs": false,
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "singleQuote": true,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "overrides": [
    {
      "files": "*.{scss,sass}",
      "options": {
        "parser": "scss"
      }
    }
  ]
}
